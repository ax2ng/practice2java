import java.util.Scanner;

public class for39 {
    public static void main(String[] args) {

        Scanner in = new Scanner(System.in);
        int a,b;
        System.out.print("A: ");
        a = in.nextInt();
        System.out.print("B: ");
        b = in.nextInt();
        for (int i = a; i <= b; ++i){
            for (int i2 = 1; i2 <= i; ++i2)
                System.out.print(i + " ");
            System.out.println();
        }
    }
}
